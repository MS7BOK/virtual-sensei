{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://kyokushin-virtual-sensei-api.onrender.com",
    "REACT_APP_WEBSOCKET_URL": "wss://kyokushin-virtual-sensei-api.onrender.com"
  }
} 